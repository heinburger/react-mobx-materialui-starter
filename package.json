{
  "name": "react-mobx-materialui-starter",
  "version": "0.2.0",
  "private": true,
  "author": "Steven Hein",
  "description": "boilerplate for react using mobx state, material-ui components, react-router, jest tests, and react-scripts",
  "keywords": [
    "react",
    "material design",
    "material-ui",
    "mobx",
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/heinburger/react-mobx-materialui-starter.git"
  },
  "license": "MIT",
  "bugs": "https://github.com/heinburger/react-mobx-materialui-starter/issues",
  "dependencies": {
    "@material-ui/core": "^1.2.3",
    "@material-ui/icons": "^1.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gh-pages": "^1.2.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mobx-react-devtools": "^5.0.1",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-event-listener": "^0.6.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.4.1",
    "source-map-explorer": "^1.5.0"
  },
  "homepage": "https://heinburger.github.io/react-mobx-materialui-starter",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
